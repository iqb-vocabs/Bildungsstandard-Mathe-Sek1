        <allg/K2/K2.2/K2.2.2>,
        <allg/K2/K2.2/K2.2.3> ;
    skos:prefLabel "Zusammenhänge  herstellen"@de .

<allg/K3> a skos:Concept ;
    skos:broader <allg> ;
